// Create a provider without service first by POST to http://34.226.221.18:8080/api/providers

{
    "providerCapacity": 5,
    "providerLogo": "s3/springfieldlogo",
    "providerEmail": "springfield@mail.com",
    "providerName": "SprngField"
}

// Then PUT the previous service link to http://34.226.221.18:8080/api/providers/1/service
{
    "_links": {
        "service": {
        "href": "http://34.226.221.18:8080/api/services/1"
        }
    }
}

